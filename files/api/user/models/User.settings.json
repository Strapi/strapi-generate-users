{
  "identity": "user",
  "connection": "default",
  "schema": true,
  "limit": 20,
  "defaultTemplate": "default",
  "displayedAttribute": "username",
  "attributes": {
    "username": {
      "type": "string",
      "minLength": 3
    },
    "email": {
      "type": "email",
      "minLength": 6
    },
    "lang": {
      "type": "string",
      "required": true
    },
    "template": {
      "type": "string",
      "required": true
    },
    "id_ref": {
      "type": "int"
    },
    "passports": {
      "collection": "passport",
      "via": "user"
    },
    "roles": {
      "collection": "role",
      "via": "users"
    }
  },
  "autoCreatedAt": true,
  "autoUpdatedAt": true,
  "migrate": "alter"
}
